{
  "source": "./src/js/",
  "destination": "./docs",
  "index": "./README.md",
  "package": "./package.json",
  "plugins": [
    {
      "name": "esdoc-publish-html-plugin",
      "option": {
        "template": "./node_modules/esdoc-custom-theme/template"
      },
  	  "manual": {
		  	"index": "./README.md",
		  	"files": [
		  		"./manual/custom_element_tracker.md",
					"./manual/custom_event.md",
		  		"./manual/CHANGELOG.md"
		  	]
		  }
    },
    {
    	"name": "esdoc-standard-plugin",
    	"option": {
    		"title": "DOMReplay",
				"coverage": {
					"enable": true
				},
				"accessor": {
					"access": ["public"],
					"autoPrivate": true
				},
	  	  "manual": {
			  	"index": "./README.md",
			  	"files": [
			  		"./manual/custom_element_tracker.md",
						"./manual/custom_event.md"
			  	]
			  }
    	}
    }
  ]
}
